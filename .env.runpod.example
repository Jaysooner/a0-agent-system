# A0 Agent System - RunPod Environment Variables
# Copy this file to .env and fill in your actual API keys

# Core AI Model API Keys
ANTHROPIC_API_KEY=your_anthropic_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
VENICE_API_KEY=your_venice_ai_api_key_here
VENICE_API_BASE=https://api.venice.ai

# AgentMail Configuration
AGENTMAIL_API_KEY=your_agentmail_api_key_here
AGENTMAIL_BASE_URL=https://api.agentmail.com

# Database Configuration (Local Assistant Pack)
POSTGRES_USER=a0
POSTGRES_PASSWORD=a0pass
POSTGRES_DB=a0memory
POSTGRES_HOST=localhost
POSTGRES_PORT=5432

# RunPod Specific
RUNPOD_API_KEY=your_runpod_api_key_here
RUNPOD_ENDPOINT_ID=your_runpod_endpoint_id_here

# Additional Service API Keys
SERPER_API_KEY=your_serper_api_key_here
BRAVE_API_KEY=your_brave_api_key_here
TAVILY_API_KEY=your_tavily_api_key_here

# Optional Local Model Configuration
LOCAL_MODEL=Llama-3.1-8B-Instruct
MODEL_PATH=/models

# Security & Access
SSH_PUBLIC_KEY=your_ssh_public_key_here
ADMIN_PASSWORD=change_this_password

# Logging & Monitoring
LOG_LEVEL=INFO
DEBUG_MODE=false

# Browser & Automation
PLAYWRIGHT_BROWSERS_PATH=/ms-playwright
CHROME_EXECUTABLE_PATH=/usr/bin/google-chrome

# Memory & Performance
MAX_MEMORY_MB=8192
MAX_WORKERS=4
CACHE_SIZE=1000

# Networking
EXPOSE_PORTS=22,80,9000-9009
TUNNEL_ENABLED=true
TUNNEL_SUBDOMAIN=your_preferred_subdomain